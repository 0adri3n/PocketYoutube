{
  "name": "pocketyoutube",
  "version": "1.3.1",
  "description": "A minimalist YouTube navigator",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dist-linux": "electron-builder --linux snap",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.pocketyoutube.app",
    "productName": "PocketYouTube",
    "asar": false,
    "directories": {
      "output": "dist"
    },
    "files": [
      "interface/**/*",
      "assets/**/*",
      "api/**/*",
      "cache/**/*",
      "index.js",
      "package.json"
    ],
    "win": {
      "target": "portable",
      "icon": "assets/pocket.ico",
      "artifactName": "PocketYouTube-${version}-portable.exe"
    }
  },
  "keywords": [
    "Youtube",
    "Electron",
    "Pocket"
  ],
  "repository": "https://github.com/0adri3n/PocketYoutube",
  "author": "0adri3n",
  "license": "ISC",
  "devDependencies": {
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "miniget": "^3.0.0",
    "scrape-youtube": "^2.4.0",
    "video.js": "^8.12.0",
    "youtube-dl-exec": "^3.0.6",
    "youtube-mp3-converter": "^1.0.3",
    "youtube-mp3-downloader": "^0.7.11",
    "ytdl-core": "^4.11.5",
    "ytdl-mp3": "^4.0.0",
    "ytmp3dl-core": "^0.0.40"
  }
}
